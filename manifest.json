{
  "name": "AutoRage",
  "version": "0.0.7",
  "description": "Replaces faces with ragefaces!",
  "permissions": [
    "http://*/*"
  ],
  "background_page": "background.html",
  "browser_action": {
      "default_icon": "AutoRage19enabled.png",
      "default_title": "Enable / Disable AutoRage"
    },
  "icons": { "16": "AutoRage16.png",
             "48": "AutoRage48.png",
            "128": "AutoRage128.png" },
  "content_scripts": [
      {
        "matches": ["http://*/*"],
        "js": ["jquery.min.js", "autorage.js"]
      }
    ]
}
